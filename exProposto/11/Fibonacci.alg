algoritmo "Nome do Programa"
var
  count, termo1, termo2, termo3 : Inteiro
inicio
   termo1 <- 0
   Escreva (termo1)
   termo2 <- 1
   Escreva (termo2)
   Para count := 3 ate 15 faca
        termo3 := termo1 + termo2
        Escreva (termo3)
        termo1 := termo2
        termo2 := termo3
   FimPara

fimalgoritmo